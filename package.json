{
  "name": "rwa-jinbao-(jbc)",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build && npx vite-bundle-analyzer dist/stats.html",
    "preview": "vite preview",
    "compile": "npx hardhat compile --config config/hardhat.config.cjs",
    "test": "npx hardhat test --config config/hardhat.config.cjs",
    "test:ui": "vitest",
    "test:ui:coverage": "vitest --coverage",
    "test:ui:watch": "vitest --watch",
    "test:contracts": "npx hardhat test --config config/hardhat.config.cjs",
    "test:coverage": "npx hardhat coverage --config config/hardhat.config.cjs",
    "test:gas": "REPORT_GAS=true npx hardhat test --config config/hardhat.config.cjs",
    "test:all": "npm run test:contracts && npm run test:ui",
    "deploy": "npx hardhat run scripts/deploy.cjs --network mc --config config/hardhat.config.cjs",
    "deploy:mc": "npx hardhat run scripts/deploy.cjs --network mc --config config/hardhat.config.cjs",
    "pages:dev": "wrangler pages dev dist --compatibility-date=2024-01-01",
    "pages:deploy": "wrangler pages deploy dist",
    "pages:deploy:optimized": "npm run build && wrangler pages deploy dist --compatibility-date=2024-01-01",
    "functions:dev": "wrangler pages dev dist --local",
    "burn:test": "curl -X POST http://localhost:8788/api/burn",
    "burn:status": "curl http://localhost:8788/api/status",
    "deploy:sepolia": "npx hardhat run scripts/deploy.cjs --network sepolia",
    "deploy:bsc": "npx hardhat run scripts/deploy.cjs --network bscTestnet",
    "deploy:local": "npx hardhat run scripts/deploy.cjs --network hardhat",
    "check": "npx hardhat run scripts/check-network.js --network mc",
    "check:mc": "npx hardhat run scripts/check-network.js --network mc",
    "check:sepolia": "npx hardhat run scripts/check-network.js --network sepolia",
    "check:bsc": "npx hardhat run scripts/check-network.js --network bscTestnet",
    "deploy:prod": "./scripts/deploy-prod.sh",
    "setup:secrets": "./scripts/setup-secrets.sh",
    "setup:secrets:prod": "./scripts/setup-secrets.sh production",
    "setup:secrets:staging": "./scripts/setup-secrets.sh staging",
    "validate:prod": "node scripts/validate-production-config.mjs",
    "pages:deploy:prod": "npm run build && wrangler pages deploy dist --project-name=jbc-ac-production",
    "pages:deploy:staging": "npm run build && wrangler pages deploy dist --project-name=jbc-ac-preview",
    "secrets:list:prod": "wrangler pages secret list --project-name=jbc-ac-production",
    "secrets:list:staging": "wrangler pages secret list --project-name=jbc-ac-preview",
    "deploy:jbc-v2": "npx hardhat run scripts/deploy-jbc-v2.cjs --network mc --config config/hardhat.config.cjs",
    "deploy:jbc-v2:sepolia": "npx hardhat run scripts/deploy-jbc-v2.cjs --network sepolia --config config/hardhat.config.cjs",
    "migrate:jbc": "npx hardhat run scripts/migrate-jbc-tokens.cjs --network mc --config config/hardhat.config.cjs",
    "test:jbc-v2": "npx hardhat test test/JBCv2.test.cjs --config config/hardhat.config.cjs",
    "test:migration": "npx hardhat test test/JBCMigration.test.cjs --config config/hardhat.config.cjs",
    "test:jbc-all": "npm run test:jbc-v2 && npm run test:migration",
    "perf:audit": "npx lighthouse http://localhost:5173 --output=html --output-path=./performance-audit.html",
    "perf:build-size": "npm run build && du -sh dist/",
    "perf:deps": "npx depcheck && npx bundle-phobia ."
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^2.2.10",
    "@tanstack/react-query": "^5.90.12",
    "dotenv": "^17.2.3",
    "ethers": "^6.8.0",
    "lucide-react": "^0.559.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.5.1",
    "viem": "^2.41.2",
    "wagmi": "^2.19.5"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.2",
    "@nomicfoundation/hardhat-ignition": "^0.15.16",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.17",
    "@nomicfoundation/hardhat-network-helpers": "^1.1.2",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.1.3",
    "@nomicfoundation/ignition-core": "^0.15.15",
    "@openzeppelin/contracts": "^5.4.0",
    "@openzeppelin/contracts-upgradeable": "^5.4.0",
    "@openzeppelin/hardhat-upgrades": "^3.9.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.20",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.16",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "chai": "^4.5.0",
    "cssnano": "^6.0.1",
    "ethers": "^6.16.0",
    "hardhat": "^2.27.0",
    "hardhat-gas-reporter": "^1.0.10",
    "jsdom": "^27.4.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "solidity-coverage": "^0.8.17",
    "terser": "^5.24.0",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "~5.8.2",
    "vite": "^4.5.14",
    "vite-plugin-compression": "^0.5.1",
    "vitest": "^4.0.16"
  },
  "optionalDependencies": {
    "@esbuild/win32-x64": "^0.27.2"
  },
  "overrides": {
    "esbuild": "^0.21.0"
  }
}
